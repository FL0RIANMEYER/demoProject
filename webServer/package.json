{
    "name": "webServer",
    "version": "1.0.0",
    "description": "",
    "main": "server/index.js",
    "author": "FASA",
    "license": "ISC",
    "watch": {
        "sync:files": "{app,server,test}/*",
        "sync:config": {
            "patterns": [
                "config"
            ]
        }
    },
    "scripts": {
        "build:dev":     "nodemon -w ./config -x \"webpack -w --env dev     --config ./config/webpack/webpack.config.babel.js\"",
        "build:devTest": "nodemon -w ./config -x \"webpack -w --env devTest --config ./config/webpack/webpack.config.babel.js\"",
        "build:test":                             "webpack --env devTest --config ./config/webpack/webpack.config.babel.js",
        "build:prod":                             "webpack --env prod    --config ./config/webpack/webpack.config.babel.js",
        "test:devTest": "mocha -w compiled/devTest/test/unit/**/*.js -r mock-css-modules -R mocha-rtp-reporter",
        "test:serviceTest": "",
        "test:systemTest": "",
        "watch": "run-p --continue-on-error watch:files watch:config",
        "watch:files": "nodemon -w ./app -w ./server -w./test -x \"npm run sync:files\"",
        "watch:config": "nodemon -w ./config -x \"npm run sync:config\"",
        "sync:files": "echo \"asd\"",
        "sync:config": "oc process -f ./config/kubernetes/resource/template.yml -p NAMESPACE=test-project -p NAME=webserver -p PROBE=/healthz -p SERVER_PORT=8080 -p LOG_LEVEL=debug -p APP_VOLUME=. | oc apply -f - && npm run sync:config:build",
        "sync:config:build": "oc start-build webserver -n test-project --from-dir . && npm run sync:config:deploy",
        "sync:config:deploy": "oc rollout latest dc/webserver && oc rollout status dc/webserver",
        "test": "npm run unit-test && npm run integration-test && npm run acceptance-test",
        "unit-test": "mocha -w compiled/out/test/unit/**/*.js        -r babel-polyfill -r babel-core/register -r mock-css-modules --reporter mocha-rtp-reporter",
        "integration-test": "mocha -w compiled/out/test/integration/**/*.js -r babel-polyfill -r babel-core/register                     --reporter mocha-rtp-reporter",
        "acceptance-test": "mocha -w compiled/out/test/acceptance/**/*.js  -r babel-polyfill -r babel-core/register                     --reporter mocha-rtp-reporter"
    },
    "pre-commit": [
        "a"
    ],
