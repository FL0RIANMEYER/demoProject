{
    "name": "webServer",
    "version": "1.0.0",
    "description": "",
    "main": "server/index.js",
    "author": "FASA",
    "license": "ISC",
    "watch": {
        "sync:files": "{app,server,test}/*",
        "sync:config": {
            "patterns": [
                "config"
            ]
        }
    },
    "scripts": {
        "start:dev": "nodemon --delay 2.5 -w ./compiled/dev     ./compiled/dev/server.js",
        "start:devTest": "nodemon --delay 1.5 -w ./compiled/devTest ./compiled/devTest/server.js",
        "start:test": "pm2 start ./compiled/test/server.js",
        "start:prod": "./compiled/test/server.js",
        "stop:test": "pm2 stop all && pm2 delete all",
        "build:dev": "nodemon -w ./config -x \"webpack -w --env dev     --config ./config/webpack/webpack.config.babel.js\"",
        "build:devTest": "nodemon -w ./config -x \"webpack -w --env devTest --config ./config/webpack/webpack.config.babel.js\"",
        "build:test": "webpack    --env test    --config ./config/webpack/webpack.config.babel.js",
        "build:prod": "webpack    --env prod    --config ./config/webpack/webpack.config.babel.js",
        "test:devTest": "mocha -w ../compiled/test/e2e/**/*.js compiled/devTest/test/unit/**/*.js -r mock-css-modules -R mocha-rtp-reporter/devTestServiceE",
        "test:devTest:service": "mocha -w compiled/devTest/test/unit/**/*.js -r mock-css-modules -R mocha-rtp-reporter/devTestServiceE",
        "test:devTest:system": "mocha -w ../compiled/test/e2e/**/*.js -R mocha-rtp-reporter/devTestSystemE",
        "test:test:service": "mocha compiled/test/test/unit/**/*.js -r mock-css-modules -R mocha-rtp-reporter/testServiceE",
        "test:test:system": "mocha ../test/e2e/test.js -r babel-polyfill -r babel-core/register -R mocha-rtp-reporter/testSystemE"
    },
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.2",
        "babel-plugin-istanbul": "^4.1.5",
        "babel-plugin-syntax-async-functions": "^6.13.0",
        "babel-plugin-transform-catch-regenerator-runtime": "^0.1.2",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "chrome-launcher": "^0.10.2",
        "concurrently": "^3.5.1",
        "css-hot-loader": "^1.3.5",
        "css-loader": "^0.28.8",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^1.1.6",
        "html-loader": "^0.5.4",
        "html-webpack-plugin": "^3.2.0",
        "ignore-styles": "^5.0.1",
        "mocha": "^5.2.0",
        "mocha-junit-reporter": "^1.17.0",
        "mocha-rtp-reporter": "0.0.36",
        "mock-css-modules": "^1.0.0",
        "node-sass": "^4.7.2",
        "npm-watch": "^0.3.0",
        "nyc": "^11.4.1",
        "optimize-css-assets-webpack-plugin": "^4.0.2",
        "pre-commit": "^1.2.2",
        "preload-webpack-plugin": "^3.0.0-alpha.3",
        "react": "^16.2.0",
        "react-hot-loader": "^3.1.3",
        "react-test-renderer": "^16.4.0",
        "sass-loader": "^6.0.6",
        "script-ext-html-webpack-plugin": "^2.0.1",
        "style-ext-html-webpack-plugin": "^3.4.7",
        "style-loader": "^0.19.1",
        "webdriverio": "^4.12.0",
        "webpack": "^4.8.3",
        "webpack-cli": "^2.1.4",
        "webpack-emit-all-plugin": "2.0.2",
        "webpack-node-externals": "^1.6.0",
        "webpack-rtp-reporter": "0.0.21", 
        "webpack-shell-plugin": "^0.5.0",
        "webpack-source-map-support": "^2.0.1"
    },
    "dependencies": {
        "babel-preset-es2017": "^6.24.1",
        "body-parser": "^1.18.2",
        "chai": "^4.1.2",
        "chai-as-promised": "^7.1.1",
        "classnames": "^2.2.5",
        "deep-diff": "^0.3.8",
        "eslint": "^4.19.1",
        "express": "^4.16.3",
        "express-handlebars": "^3.0.0",
        "istanbul": "^0.4.5",
        "load-source-map": "^1.0.0",
        "mockery": "^2.1.0",
        "mongodb": "^3.0.8",
        "mustache": "^2.3.0",
        "nodegit": "^0.22.0",
        "nodemon": "^1.17.4",
        "npm-run-all": "^4.1.3",
        "pm2": "^2.10.4",
        "purify-css": "^1.2.5",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.7",
        "recompose": "^0.26.0",
        "redux": "^4.0.0",
        "request": "^2.83.0",
        "reselect": "^3.0.1",
        "sinon": "^4.1.3",
        "sinon-chai": "^2.14.0",
        "source-map": "^0.6.1",
        "source-map-resolve": "^0.5.1",
        "superagent": "^3.8.3"
    }
}
