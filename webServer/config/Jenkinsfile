pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                println(new Date().getTime())
                sh "pwd"
                dir('webServer') {
                    sh "pwd"
                    sh "node --version"
                    sh "npm run"
                    sh "npm run build:devTest"
                }
                println(new Date().getTime())
            }
        }

        stage('deploy') {
            steps {
                println(new Date().getTime())
                sh "npm run test:devTest"
                println(new Date().getTime())
            }
        }
    }
}
